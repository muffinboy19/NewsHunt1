<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout

    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layout"
    android:layout_width="match_parent"
    android:layout_height="729dp"
    android:background="#FAFAFA"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="1.0"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent">


    <SearchView
        android:id="@+id/searchViewCollegeNews"
        style="@style/CustomSearchViewStyle"
        android:layout_width="320dp"
        android:layout_height="36dp"
        android:layout_marginHorizontal="0dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/search_bar"
        android:elevation="10dp"
        android:queryBackground="@color/transparent"
        android:queryHint="Search..."
        android:theme="@style/CustomSearchViewStyle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="88dp"
        android:layout_marginTop="253dp"
        android:fontFamily="@font/popinsss"
        android:text="Comming Soon..."
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/searchViewCollegeNews" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottom_navigationCollegeNews"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:background="@color/def"
        app:layout_constraintBottom_toBottomOf="parent"
        app:menu="@menu/nav_bar"
        tools:layout_editor_absoluteX="90dp" />

</androidx.constraintlayout.widget.ConstraintLayout>



